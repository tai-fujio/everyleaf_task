<h1>タスク一覧</h1>
  <table>
    <tr>
      <th>タスク名</th>
      <th>タスク詳細</th>
    </tr>
    <tr>
      <%@tasks.each do |t|%>
        <td><%= t.task_name%></td>
        <td><%= t.task_details%></td>
        <td><%= link_to "詳細",task_path(t.id)%></td>
        <td><%= link_to "編集",edit_task_path(t.id)%></td>
        <td><%= link_to "削除",task_path(t.id),method: :delete%></td>
      <% end %>  
    </tr>
  </table>
  <%=link_to "新規作成",new_task_path%>
